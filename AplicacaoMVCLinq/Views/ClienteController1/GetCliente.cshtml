@{
    ViewData["Title"] = "Listar Alunos";
}
<h1>Listar Alunos</h1>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>Data Nascimento</th>
        </tr>
    </thead>

    <tbody>
        <!-- Laço de repetição foreach para listar todos os elementos da lista de cliente-->
        @foreach (var cliente in Model)
        {
            <tr>
                <td>@cliente.ClienteId</td>
                <td>@cliente.Nome</td>
                <td>@cliente.Email</td>
                <td>@cliente.Telefone</td>
                <td>@cliente.DataNasc</td>
            </tr>
        }
    </tbody>
</table>